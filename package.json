{
  "name": "@octetstream/eslint-config",
  "version": "7.2.1",
  "description": "AirBnb-based ESlint config, tweaked for my needs.",
  "author": "Nick K.",
  "license": "MIT",
  "exports": {
    ".": "./lib/configs/base.js",
    "./esm": "./lib/configs/esm.js",
    "./react": "./lib/configs/react.js",
    "./typescript": "./lib/configs/typescript.js",
    "./typescript/react": "./lib/configs/typescript/react.js",
    "./typescript/esm": "./lib/configs/typescript/esm.js",
    "./typescript/esm/react": "./lib/configs/typescript/esm/react.js",
    "./lib/rules": null,
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "repository": "octet-stream/eslint-config",
  "scripts": {
    "build": "unbuild",
    "postinstall": "pnpm build",
    "test": "pnpm build && cross-env NODE_OPTIONS=\"--no-warnings --import tsx\" ava",
    "prepare": "npx is-in-ci || husky install"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser",
        "eslint-config-airbnb-typescript",
        "eslint-import-resolver-typescript",
        "eslint-plugin-jsx-a11y",
        "eslint-plugin-react",
        "eslint-plugin-react-hooks",
        "eslint-config-airbnb",
        "typescript"
      ]
    }
  },
  "dependencies": {
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-ava": "14.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-promise": "6.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": ">= 3.32.x < 8.x || 8.x",
    "eslint-config-airbnb": "19.x",
    "eslint-config-airbnb-typescript": "17.x",
    "eslint-import-resolver-typescript": "3.x",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": ">= 4.7.x < 5.x || 5.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@types/eslint": "8.44.9",
    "@types/node": "20.10.4",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "ava": "6.0.1",
    "cross-env": "7.0.3",
    "dedent": "1.5.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.0",
    "lint-staged": "15.2.0",
    "tsx": "4.6.2",
    "typescript": "5.3.3",
    "unbuild": "2.0.0"
  }
}
