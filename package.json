{
  "name": "@octetstream/eslint-config",
  "version": "7.0.2",
  "description": "AirBnb-based ESlint config, tweaked for my needs.",
  "author": "Nick K.",
  "license": "MIT",
  "main": "./base.js",
  "exports": {
    ".": "./base.js",
    "./react": "./react.js",
    "./typescript": "./typescript/typescript.js",
    "./typescript/react": "./typescript/react.js",
    "./esm": "./esm/esm.js",
    "./esm/react": "./esm/react.js",
    "./esm/typescript": "./esm/typescript/typescript.js",
    "./esm/typescript/react": "./esm/typescript/react.js",
    "./package.json": "./package.json"
  },
  "repository": "octet-stream/eslint-config",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser",
        "eslint-config-airbnb-typescript",
        "eslint-import-resolver-typescript",
        "eslint-plugin-jsx-a11y",
        "eslint-plugin-react",
        "eslint-plugin-react-hooks",
        "eslint-config-airbnb",
        "typescript"
      ]
    }
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": ">= 3.32.x < 8.x || 8.x",
    "eslint-config-airbnb": "19.x",
    "eslint-config-airbnb-typescript": "17.x",
    "eslint-import-resolver-typescript": "3.x",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": ">= 4.7.x < 5.x || 5.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.0.3"
  }
}
